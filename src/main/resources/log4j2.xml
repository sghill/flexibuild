<Configuration name="flexibuild" status="ERROR">
    <Appenders>
        <Console name="Out">
            <PatternLayout pattern="[%c{1}] %d{HH:mm:ss.SSS} %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Out"/>
        </Root>
    </Loggers>
</Configuration>
